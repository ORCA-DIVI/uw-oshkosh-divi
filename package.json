{
  "name": "uw-oshkosh-divi",
  "version": "0.2.7",
  "description": "UW-Oshkosh child theme for Divi",
  "main": "index.js",
  "scripts": {
    "dev:copy-php": "cd src && copy **/**/*.php ../builds/dev && copy **/*.php ../builds/dev",
    "dev:sass": "node-sass --source-map true --output-style expanded src/uw-oshkosh-divi/style.scss --output builds/dev/uw-oshkosh-divi",
    "dev:js-transform": "mkdirp builds/dev/uw-oshkosh-divi/js && babel src/uw-oshkosh-divi/js/**/*.js --out-file builds/dev/uw-oshkosh-divi/js/custom.js --source-maps ",
    "img:dev:compress-favicons": "imagemin src/uw-oshkosh-divi/img/favicons/*.* --out-dir=builds/dev/uw-oshkosh-divi/img/favicons --plugin=jpeg-recompress --plugin=svgo",
    "img:dev:compress-footer": "imagemin src/uw-oshkosh-divi/img/footer/*.* --out-dir=builds/dev/uw-oshkosh-divi/img/footer --plugin=jpeg-recompress --plugin=svgo",
    "img:dev:rest": "imagemin src/uw-oshkosh-divi/img/screenshot.png --out-dir=builds/dev/uw-oshkosh-divi/img --plugin=jpeg-recompress --plugin=svgo",
    "img:dev:screenshot": "imagemin src/uw-oshkosh-divi/*.* --out-dir=builds/dev/uw-oshkosh-divi/ --plugin=jpeg-recompress --plugin=svgo",
    "dev:img-compress": "npm-run-all --parallel img:dev:*",
    "build-dev": "npm-run-all --parallel dev:*",
    "prod:copy-php": "cd src && copy **/**/*.php ../builds/prod && copy **/*.php ../builds/prod",
    "prod:sass": "node-sass --output-style compressed src/uw-oshkosh-divi/style.scss --output builds/prod/uw-oshkosh-divi",
    "prod:js-transform": "mkdirp builds/prod/uw-oshkosh-divi/js && babel src/uw-oshkosh-divi/js/**/*.js --out-file builds/prod/uw-oshkosh-divi/js/custom.js",
    "img:prod:compress-favicons": "imagemin src/uw-oshkosh-divi/img/favicons/*.* --out-dir=builds/prod/uw-oshkosh-divi/img/favicons --plugin=jpeg-recompress --plugin=svgo",
    "img:prod:compress-footer": "imagemin src/uw-oshkosh-divi/img/footer/*.* --out-dir=builds/prod/uw-oshkosh-divi/img/footer --plugin=jpeg-recompress --plugin=svgo",
    "img:prod:rest": "imagemin src/uw-oshkosh-divi/img/*.* --out-dir=builds/prod/uw-oshkosh-divi/img --plugin=jpeg-recompress --plugin=svgo",
    "img:prod:screenshot": "imagemin src/uw-oshkosh-divi/screenshot.png --out-dir=builds/prod/uw-oshkosh-divi/ --plugin=jpeg-recompress --plugin=svgo",
    "prod:img-compress": "npm-run-all --parallel img:prod:*",
    "build-prod": "npm-run-all --parallel prod:*"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uwosh/uw-oshkosh-divi.git"
  },
  "keywords": [
    "child",
    "theme",
    "Divi",
    "WordPress",
    "Theme"
  ],
  "author": "Joseph Kerkhof",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/uwosh/uw-oshkosh-divi/issues"
  },
  "homepage": "https://github.com/uwosh/uw-oshkosh-divi#readme",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "copy": "^0.3.0",
    "imagemin": "^5.2.2",
    "imagemin-cli": "^3.0.0",
    "imagemin-jpeg-recompress": "^5.1.0",
    "imagemin-svgo": "^5.2.0",
    "node-sass": "^3.10.1",
    "npm-run-all": "^3.1.0"
  }
}
